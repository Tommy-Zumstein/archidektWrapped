{
  "name": "archidekt",
  "version": "0.0.14",
  "private": false,
  "description": "archidekt api",
  "keywords": [
    "archidekt",
    "mtg",
    "magic",
    "magic the gathering",
    "Magic: the Gathering",
    "tcg",
    "trading",
    "card",
    "game",
    "archideck",
    "deck",
    "api",
    "axios",
    "MtG"
  ],
  "author": {
    "name": "Sumner Magruder",
    "email": "sumner.magruder@zmnh.uni-hamburg.de",
    "url": "https://gitlab.com/SumNeuron/archidekt/"
  },
  "license": "MIT",
  "repository": "gitlab:SumNeuron/archidekt",
  "bugs": {
    "url": "https://gitlab.com/SumNeuron/archidekt/issues",
    "email": "sumner.magruder@zmnh.uni-hamburg.de"
  },
  "homepage": "https://gitlab.com/SumNeuron/archidekt#README",
  "main": "dist/archidekt.umd.js",
  "module": "dist/archidekt.esm.js",
  "unpkg": "dist/archidekt.min.js",
  "browser": {
    "./sfc": "src/archidekt.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/archidekt.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/archidekt.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/archidekt.min.js",
    "push": "git add .; git commit -m 'latest'; git push;",
    "all": "npm run build; npm publish; npm run push;"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.4.3",
    "axios": "^0.18.0",
    "babel-plugin-wildcard": "^5.0.0",
    "minimist": "^1.2.0",
    "rollup": "^0.66.2",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-uglify-es": "0.0.1"
  }
}
